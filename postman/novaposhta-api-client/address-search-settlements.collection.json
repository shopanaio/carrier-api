      "name": "Search Settlements - Київ",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"apiKey\": \"{{API_KEY}}\",\n  \"modelName\": \"Address\",\n  \"calledMethod\": \"searchSettlements\",\n  \"methodProperties\": {\n    \"CityName\": \"Київ\",\n    \"Limit\": \"50\",\n    \"Page\": \"1\"\n  }\n}"
        },
        "url": {
          "raw": "{{BASE_URL}}",
          "host": ["{{BASE_URL}}"]
        },
        "description": "Search for settlements matching 'Київ'. Returns settlement name, region, number of warehouses. All parameters are required."
      },
      "response": []
    },
