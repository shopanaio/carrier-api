{
  "name": "@shopana/novaposhta-transport-fetch",
  "version": "0.0.1-alpha.0",
  "description": "Fetch-based HTTP JSON transport for Nova Poshta API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "dev": "tsc -w -p tsconfig.json",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nova-poshta",
    "novaposhta",
    "transport",
    "http-transport",
    "http-client",
    "fetch",
    "cross-fetch",
    "api-client",
    "rest",
    "shipping",
    "carrier",
    "typescript",
    "nodejs",
    "browser",
    "esm"
  ],
  "author": "Shopana.io",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/shopanaio/carrier-api.git",
    "directory": "packages/novaposhta-transport-fetch"
  },
  "bugs": {
    "url": "https://github.com/shopanaio/carrier-api/issues"
  },
  "homepage": "https://github.com/shopanaio/carrier-api/tree/main/packages/novaposhta-transport-fetch#readme",
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false
}
